# BEGIN Better WP Security
Options -Indexes

<files .htaccess>
Order allow,deny
Deny from all
</files>

<files readme.html>
Order allow,deny
Deny from all
</files>

<files readme.txt>
Order allow,deny
Deny from all
</files>

<files install.php>
Order allow,deny
Deny from all
</files>

<files wp-config.php>
Order allow,deny
Deny from all
</files>

<IfModule mod_rewrite.c>
RewriteEngine On

RewriteRule ^wp-admin/includes/ - [F,L]
RewriteRule !^wp-includes/ - [S=3]
RewriteCond %{SCRIPT_FILENAME} !^(.*)wp-includes/ms-files.php
RewriteRule ^wp-includes/[^/]+\.php$ - [F,L]
RewriteRule ^wp-includes/js/tinymce/langs/.+\.php - [F,L]
RewriteRule ^wp-includes/theme-compat/ - [F,L]

RewriteCond %{REQUEST_METHOD} ^(TRACE|DELETE|TRACK) [NC]
RewriteRule ^(.*)$ - [F,L]

RewriteRule ^login/?$ /wp-login.php?5o4gite1dqbnodm2csmhl [R,L]

RewriteCond %{HTTP_COOKIE} !^.*wordpress_logged_in_.*$
RewriteRule ^admin/?$ /wp-login.php?5o4gite1dqbnodm2csmhl&redirect_to=/wp-admin/ [R,L]

RewriteRule ^admin/?$ /wp-admin/?5o4gite1dqbnodm2csmhl [R,L]

RewriteRule ^register/?$ /wp-login.php?5o4gite1dqbnodm2csmhl&action=register [R,L]

RewriteCond %{SCRIPT_FILENAME} !^(.*)admin-ajax\.php
RewriteCond %{HTTP_REFERER} !^(.*)tappmd.com/wp-admin
RewriteCond %{HTTP_REFERER} !^(.*)tappmd.com/wp-login\.php
RewriteCond %{HTTP_REFERER} !^(.*)tappmd.com/login
RewriteCond %{HTTP_REFERER} !^(.*)tappmd.com/admin
RewriteCond %{HTTP_REFERER} !^(.*)tappmd.com/register
RewriteCond %{QUERY_STRING} !^5o4gite1dqbnodm2csmhl
RewriteCond %{QUERY_STRING} !^action=logout
RewriteCond %{QUERY_STRING} !^action=rp
RewriteCond %{QUERY_STRING} !^action=register
RewriteCond %{QUERY_STRING} !^action=postpass
RewriteCond %{HTTP_COOKIE} !^.*wordpress_logged_in_.*$
RewriteRule ^.*wp-admin/?|^.*wp-login\.php /not_found [R,L]

RewriteCond %{QUERY_STRING} ^loggedout=true
RewriteRule ^.*$ /wp-login.php?5o4gite1dqbnodm2csmhl [R,L]
</IfModule>
# END Better WP Security

<files wp-config.php>
order allow,deny
deny from all
</files>

